= form_for :user, url: users_path do |f|
  Email:
  = f.email_field :email
  %br
  Password:
  = f.password_field :password
  %br
  Confirm Password:
  = f.password_field :confirm_password
  %br
  Date of Birth:
  = f.date_select :date_of_birth
  %br
  Gender:
  = f.select :gender_type, options_for_select([["Select One", ""], "Male", "Female"])

  = f.submit
